# print(soup.prettify())	# to print html in tree structure

# print(soup.find_all("div", class_="location-text para-text"))
mdiv = soup.find('div',class_="selectize-dropdown-content")
namediv = mdiv.find_all('div', class_="option-name")
adrdiv = mdiv.findAll('div', class_="option-address",text=True)
faddr = [row.text for row in adrdiv]
fname = [row.text for row in namediv]
print(faddr)
print(fname)
print(len(fname))
print(fname[0])

p = 0  # initial place in array
l = len(fname) - 1
flat = []
flong = []
f = open('noodel20_data.csv', 'a')
f.write("Name,Address,latitude,Longitude\n")
while p < l:
    for adr in faddr:
        try:
            address = adr.split(",")
            locator = Nominatim(user_agent="myGeocoder")
            location = locator.geocode(address[-1:])
            x = location.latitude
            y = location.longitude
            f.write(str(fname[p]) + ", ")
            f.write(str(x) + ", ")
            f.write(str(y) + ", ")
            f.write(str(faddr[p]) + "\n")
            p = p + 1
        except:
            print(fname[p], faddr[p])

f.close()  # close file
